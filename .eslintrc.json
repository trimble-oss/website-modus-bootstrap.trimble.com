{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true
  },
  "extends": ["eslint:recommended"],
  "plugins": ["html"],
  "rules": {
    // override configuration set by extending "eslint:recommended"
    "no-undef": "off",
    "no-unused-vars": "off",
    "no-useless-escape": "off"
  }
}
